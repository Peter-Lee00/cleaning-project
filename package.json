{
  "name": "cleaning-project",
  "version": "1.0.0",
  "description": "A platform connecting home owners with cleaning service providers",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && node server.js",
    "build": "npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && node server.js",
    "lint": "npm run lint:backend",
    "lint:backend": "cd backend && npm run lint",
    "format": "npm run format:backend",
    "format:backend": "cd backend && npm run format",
    "generate-data": "cd backend && npm run generate-data",
    "setup": "npm run install:all && npm run generate-data"
  },
  "keywords": [
    "cleaning",
    "services",
    "booking",
    "platform"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
